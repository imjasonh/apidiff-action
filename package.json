{
  "author": "Jason Hall",
  "dependencies": {
    "@actions/core": "^2.0.2",
    "@actions/exec": "^2.0.0",
    "@actions/github": "^7.0.0"
  },
  "description": "GitHub Action for detecting breaking API changes in Go code",
  "devDependencies": {
    "@vercel/ncc": "^0.38.4",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.3.1",
    "prettier": "^3.8.1"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "keywords": [
    "github-action",
    "go",
    "golang",
    "api",
    "compatibility",
    "breaking-changes"
  ],
  "license": "Apache-2.0",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,yml}": [
      "prettier --write"
    ]
  },
  "main": "index.js",
  "name": "apidiff-action",
  "scripts": {
    "build": "ncc build index.js -o dist",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepare": "husky install",
    "test": "node --test"
  },
  "version": "1.0.0"
}
